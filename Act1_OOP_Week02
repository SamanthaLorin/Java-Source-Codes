package com.mycompany.javaacts;

class TrainLine{
        String lineName;

        public TrainLine() {
            lineName = "Unknown Train Line";
        }

        public void displayLine() {
            System.out.println("Train Line: " + lineName);
        }
    
    }

public class JavaActs {

    public static void main(String[] args) {
        TrainLine train = new TrainLine();
        train.displayLine();
    }
}

OUTPUT:
Train Line: Unknown Train Line

------------------

package com.mycompany.javaacts;

class Person {
    private String name;

    public void setName(String n) {
        name = n;
    }

    public String getName() {
        return name;
    }
}

public class JavaActs {
    public static void main(String[] args) {
        Person p = new Person();
        p.setName("Alice");
        System.out.println("Name: " + p.getName());
    }
}

OUTPUT:
Name: Alice
